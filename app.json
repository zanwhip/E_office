{
  "expo": {
    "name": "EOffice",
    "slug": "eoffice",
    "version": "1.1.0",
    "scheme": "exp://com.eoffice.app.exp.direct//redirect",
    "sdkVersion": "48.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eoffice.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.eoffice.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5a4fbef5-a3e1-4c47-8c73-48faccdb2984"
      }
    },
  
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "zanwhip",
            "project": "eoffice",
            "authToken": "exp://com.eoffice.app.exp.direct//redirect"
          }
        }
      ]
    }
  }
}
